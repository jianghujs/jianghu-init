{
  "pageId": "studentManagement",
  "pageType": "1table-page",
  "table": "student",
  "title": "学生管理",
  "pageHeader": [{
      "breadcrumbs": ["首页", "学生管理"]
    },{
      "type": "v-select",
      "label": "性别",
      "align": "right",
      "props": {
        "items": ["全部", "男", "女"]
      }
  }],
  "tableHeader": [
    {
      "type": "v-btn",
      "label": "新增",
      "props": {
        "class": "v-middle"
      }
    }
  ],
  "tableFooter": [ "pagination" ],
  "columns": [
    {
      "name": "id",
      "label": "ID",
      "type": "v-text-field",
      "width": 20,
      "sortable": true,
      "searchable": false
    },
    {
      "name": "gender",
      "label": "性别",
      "type": "v-text-field",
      "required": true,
      "sortable": true,
      "searchable": true,
      "rules": [
        "v => !!v || '此项必填'",
        "v => !!v != 2 || '此项必须===2'"
      ]
    },
    {
      "name": "classId",
      "label": "班级",
      "type": "v-text-field",
      "required": false,
      "sortable": true,
      "searchable": false
    },
    {
      "type": "action",
      "label": "操作",
      "width": 100,
      "actions": [
        {
          "type": "v-btn",
          "label": "修改"
        },
        {
          "type": "v-btn",
          "label": "删除"
        }
      ]
    }
  ]
}