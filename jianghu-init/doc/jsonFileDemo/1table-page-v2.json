{
  "pageType": "1table-page",
  "pageId": "studentManagement",
  "table": "student",
  "pageHeader": {
    "title": "studentManagement",

    "pageDocEnable": true,
    "pageDocSrc": "studentManagement.md",

    "backendSearch": {
      "submitBtn": "搜索",
      "items": [{
        "type": "v-select",
        "label": "性别",
        "props": {
          "items": ["全部","男","女"]
        }
      }]
    }
  },
  "tableHeader": {
    "btnList": [
      {
        "title": "新增",
        "uiActionId": "createItem",
        "uiActionData": {}
      },
      {
        "title": "导出",
        "uiActionId": "exportItem",
        "uiActionData": {}
      },
      {
        "title": "导入",
        "uiActionId": "importItem",
        "uiActionData": {}
      }
    ],
    "searchIgnoreFields": [
      "name",
      "title"
    ]
  },
  "tableBody": {
    "operationPosition": "left",
    "operationBtnList": [
      {
        "title": "修改",
        "uiActionId": "updateItem",
        "uiActionData": {}
      },
      {
        "title": "删除",
        "uiActionId": "deleteItem",
        "uiActionData": {}
      }
    ],
    "zebraLineEnable": true
  },
  "tableFooter": {
    "itemsPerPageText": "每页",
    "itemsPerPageOptions": [20,50,-1],
    "itemsPerPageAllText": "所有"
  },
  "columns": [
    {
      "name": "id",
      "label": "ID",
      "type": "v-text-field",
      "width": 20,
      "align": "center",
      "fixed": true,
      "createEnable": false,
      "updateEnable": false,
      "sortable": true,
      "searchable": false,
      "props": {
        "color": "red"
      }
    },
    {
      "name": "studentId1",
      "label": "学生ID1"
    },
    {
      "name": "studentId",
      "label": "学生ID"
    },
    {
      "name": "gender",
      "label": "性别",
      "type": "v-select",
      "required": true,
      "sortable": true,
      "searchable": true,
      "items": ["全部", "男", "女"],
      "props": {
        "color": "red"
      }
    },
    {
      "name": "classId",
      "label": "班级",
      "type": "v-text-field",
      "required": true,
      "sortable": true,
      "searchable": false,
      "rules": [
        "v => v.length > 10 || 'classId must be less than 10 characters'",
        "v => _.startsWith(v, 'S') || 'classId must be start with S'"
      ],
      "props": {
        "color": "red"
      }
    },
    {
      "name": "dateOfBirth",
      "label": "生日",
      "type": "v-date-picker",
      "props": {}
    }
  ]
}